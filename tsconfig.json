{
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "commonjs",                                
    "allowJs": true,
    "checkJs": false,
    "jsx": "react",
    "outDir": "./build",
    "removeComments": true,
    "noEmit": true,
    "pretty": true,
    "moduleResolution": "node",
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,
    "strict": true, 
    "skipLibCheck": true 
  }
}
